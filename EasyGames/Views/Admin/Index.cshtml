@model IEnumerable<EasyGames.Models.ApplicationUser>

@{
    ViewData["Title"] = "User Management";
}

<h1>User Management</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Email</th>
            <th>UserName</th>
            <th>Id</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.Email</td>
            <td>@user.UserName</td>
            <td>@user.Id</td>
        </tr>
    }
    </tbody>
</table>
